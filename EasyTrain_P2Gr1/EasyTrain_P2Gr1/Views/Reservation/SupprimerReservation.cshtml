@{
}
@model EasyTrain_P2Gr1.Models.Reservation
@{
    Layout = "_LayoutGestionnaire";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>SupprimerReservation</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        <fieldset>
            <legend>Supprimer une Réservation : </legend>
            <br />
            @Html.LabelFor(m => Model.Client)
            @Html.TextBoxFor(m => Model.Client)
            @Html.ValidationMessageFor(m => Model.Client)
            <br />
            @Html.LabelFor(m => Model.CoursProgramme)
            @Html.TextBoxFor(m => Model.CoursProgramme)
            @Html.ValidationMessageFor(m => Model.CoursProgramme)
            <br />
            <input type="submit" value="Supprimer" />
        </fieldset>
    }
</body>
</html>



