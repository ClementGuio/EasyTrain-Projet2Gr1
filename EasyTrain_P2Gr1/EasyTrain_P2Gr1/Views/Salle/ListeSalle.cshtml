@model List< EasyTrain_P2Gr1.Models.Salle>
@{
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ListeSalle</title>
</head>
<body>
<table border="1">
    <tr>
        <th>Id</th>
        <th>Nom</th>
        <th>Type</th>
        <th>Equipements</th>
    </tr>
    @foreach (EasyTrain_P2Gr1.Models.Salle salle in Model)
    {
        <tr>
            <td>@salle.Id</td>
            <td>@salle.Nom</td>
            <td>@salle.Type</td>
                <td>@salle.Equipements</td>
        </tr>
    }
</table>
</body>

</html>

