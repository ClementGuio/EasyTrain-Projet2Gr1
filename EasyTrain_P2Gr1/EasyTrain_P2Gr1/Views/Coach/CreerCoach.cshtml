@model EasyTrain_P2Gr1.Models.Coach
@{
    @if (ViewData["role"].Equals("Coach"))
    {
        Layout = "_LayoutCoach";
    }
    else if (ViewData["role"].Equals("Gestionnaire"))
    {
        Layout = "_LayoutGestionnaire";
    }
    else
    {
        Layout = "_LayoutClient";
    }
}

<div class="fieldsetModifier">
    <fieldset>
        <legend><h2>Ajouter un coach : </h2></legend>
        @using(Html.BeginForm()){
        @Html.Partial("_CreerUtilisateur")
        
        <div class="btnCoachModifier">

            <input type="button" value="Retour" onclick=" history.back();" class="btnStandard" />
            <input type="submit" value="Créer" class="btnConfirmer" />
                
        </div>
        }
    </fieldset>
</div>