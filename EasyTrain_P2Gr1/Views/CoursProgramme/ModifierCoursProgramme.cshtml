@model EasyTrain_P2Gr1.ViewModels.FormulaireCoursProgrammeViewModel
@{
    Layout = "_LayoutCoach";
}
@using(Html.BeginForm())
{
    @Html.LabelFor(c => c.DateDebut)
    @Html.TextBoxFor(c => c.DateDebut)
    <br />
    @Html.LabelFor(c => c.DateFin)
    @Html.TextBoxFor(c => c.DateFin)
    <br />
   
    <br />
    @Html.DropDownListFor(m => m.CoursId, new SelectList(Model.Cours, "Id", "Titre"))
    
    <input type="submit" value="Valider">
}